generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room{
  id          Int    @id @default(autoincrement())
  admin       Int    @unique
  roomCode    Int    @unique
  roomName    String
  password    String?
  isPrivate   Boolean
  lastActivity DateTime
  createdAt   DateTime  @default(now())
}